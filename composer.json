{
  "name": "spryker/shopping-list-product-option-connector",
  "description": "ShoppingListProductOptionConnector module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/kernel": "^3.0.0",
    "spryker/product-option": "^6.4.0",
    "spryker/shopping-list": "^1.1.0",
    "spryker/shopping-list-extension": "^1.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/company": "*",
    "spryker/company-business-unit": "*",
    "spryker/company-user": "*",
    "spryker/customer": "*",
    "spryker/permission": "*",
    "spryker/product": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
